syntax on

"Indent
set smartindent
set autoindent

set nocompatible
set number
set title
set tabstop=4
set shiftwidth=4
set expandtab
set smarttab

set showmatch "parenthesis pair
set hlsearch
set ignorecase
set incsearch
set wildmenu
set mouse=a
set completeopt=menuone,longest
set showcmd
set timeoutlen=10
set clipboard=unnamed
set statusline=\ %F"\%<%l:%v\ [%P]%=%a\ %h%m%r\ 
set laststatus=2
